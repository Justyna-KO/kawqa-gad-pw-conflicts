{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:playwright/playwright-test",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier", "eslint-plugin-playwright", "eslint-plugin-prettier"],
  "rules": {
    "no-console": "error",
    "prettier/prettier": "warn"
  }
}
